#include <iostream>
using namespace std;

void myswap(double& x, double& y) {
    double tmp;
    tmp = x;
    x = y;
    y = tmp;
}

int main() {
    double a,b;
    cout << "두 수 입력: ";
    cin >> a >> b;
    cout << "a: " << a << ", b: " << b << endl;
    
    myswap(a,b);
    cout << "a: " << a << ", b: " << b << endl;
    
    return 0;
}
