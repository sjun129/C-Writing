#include <iostream>
#include <fstream>
#include <algorithm>
using namespace std;

int main()
{
    double arr[31];
    ifstream fin("sort_before.txt");
    ofstream fout("sort_after.txt");
    
    for(int i = 0; i < 31; i++) fin >> arr[i];
    sort(arr,arr+31,greater<double>() );
    for(int i = 0; i < 31; i++) fout << arr[i] << endl;
    
    
    fin.close();
    return 0;
}
